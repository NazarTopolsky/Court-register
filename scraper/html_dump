<script type="text/javascript">
    jQuery(function ($) {
        $(document).ready(function () {
            var ti = 1509041;
            var pp = 250;
            var tp = Math.ceil(((ti > 100000)?100000:ti)/pp);
            var cp = 1;
            var bp = 3 + 7 + 3;
            function htmlButton(href, bClass, text) { return '<a ' + href + ' class="' + bClass + '">' + text + '</a>'; }
            var html = '';
            html += htmlButton((cp != 1)?'href="/Page/1"':'',              (cp != 1)?'enButton':'disButton', '«');
            html += htmlButton((cp != 1)?'href="/Page/' + (cp-1) + '"':'', (cp != 1)?'enButton':'disButton', '<' );
            if (tp <= bp) {
                for (var i = 1; i <= tp; i++) html += htmlButton((i != cp)?'href="/Page/' + i + '"':'', (i != cp)?'enButton':'disButton', i);
            } else
            if (cp <= Math.ceil(bp/2)) {
                for (var i = 1; i <= bp-3; i++) html += htmlButton((i != cp)?'href="/Page/' + i + '"':'', (i != cp)?'enButton':'disButton', i);
                html += htmlButton('', 'dotButton', '...');
                for (var i = tp-1; i <= tp; i++) html += htmlButton('href="/Page/' + i + '"', 'enButton', i);
            } else
            if (cp > tp - Math.ceil(bp/2)) {
                for (var i = 1; i <= 2; i++) html += htmlButton('href="/Page/' + i + '"', 'enButton', i);
                html += htmlButton('', 'dotButton', '...');
                for (var i = tp - (bp-3) + 1; i <= tp; i++) html += htmlButton((i != cp)?'href="/Page/' + i + '"':'', (i != cp)?'enButton':'disButton', i);
            } else {
                for (var i = 1; i <= 2; i++) html += htmlButton('href="/Page/' + i + '"', 'enButton', i);
                html += htmlButton('', 'dotButton', '...');
                for (var i = cp - Math.round((bp-7)/2); i <= cp + Math.round((bp-7)/2); i++) html += htmlButton((i != cp)?'href="/Page/' + i + '"':'', (i != cp)?'enButton':'disButton', i);
                html += htmlButton('', 'dotButton', '...');
                for (var i = tp-1; i <= tp; i++) html += htmlButton('href="/Page/' + i + '"', 'enButton', i);
            }
            html += htmlButton((cp != tp)?'href="/Page/' + (cp+1) + '"':'', (cp != tp)?'enButton':'disButton', '>' );
            html += htmlButton((cp != tp)?'href="/Page/' + tp + '"':'',     (cp != tp)?'enButton':'disButton', '»');
            $('#divTopNavigate').html(html);
            $('#divTopNavigate').find('a.enButton').live('click', function () {blockPage();});
            $('#divBottomNavigate').html(html);
            $('#divBottomNavigate').find('a.enButton').live('click', function () {blockPage();});
        });
    });        
</script>

